screen -t simulator-8010 bash
stuff "java -jar $KINETIC_JAR 8010 /srv/node1/sdb1/kinetic 8410"
screen -t simulator-8020 bash
stuff "java -jar $KINETIC_JAR 8020 /srv/node2/sdb2/kinetic 8420"
screen -t simulator-8030 bash
stuff "java -jar $KINETIC_JAR 8030 /srv/node3/sdb3/kinetic 8430"
